(*Suma:*)
(*5*)

MODULE seis;

  VAR suma : INTEGER;
  		a1:INTEGER;
  		b1:INTEGER;
  
  PROCEDURE Sumar (VAR a: INTEGER; VAR b: INTEGER) : INTEGER;
  	VAR s: INTEGER;
 	BEGIN
 		s := a+b;
 		RETURN s; 	
 	END Sumar; 	
  
  BEGIN
    a1:=2;
    b1:=3;
    
    suma := Sumar(a1,b1);
    WRITESTRING("Suma:");
    WRITEINT(suma);
    
  END seis;
