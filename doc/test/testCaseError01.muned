MODULE errorSemanticoUno;

  (* pruebas con operaciones *)

(* se tiene que detectar error semántico al intentar usar AND con un booleano y un entero*)

  CONST A = 10;
	B = 20;

  VAR 
	a1:INTEGER;
	b1:INTEGER;
	bol1 : BOOLEAN;
	
	bol3 : BOOLEAN;
  BEGIN

    a1:=A;
    b1:=2;

    

    
    bol1:=TRUE;
    (*a continuación se tiene que detectar el error semántico*)
    bol3:=(bol1 AND a1);
	    
  END errorSemanticoUno;
