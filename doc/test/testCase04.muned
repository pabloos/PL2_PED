MODULE cuatro;

  (* pruebas con operaciones *)

(* se tiene que imprimir un 5*)

  CONST A = 10;
	B = 20;

  VAR division : INTEGER;
	a1:INTEGER;
	b1:INTEGER;
	bol1 : BOOLEAN;
	bol2 : BOOLEAN;
	bol3 : BOOLEAN;
  BEGIN

    a1:=A;
    b1:=2;

    division:=a1/b1;

    WRITEINT(division);
    
	bol1:=TRUE;
	bol2:=FALSE;
	bol3:=(bol1 AND bol2);
	    
  END cuatro;
